Host vm-rmsv
	HostName 20.57.185.3
	User azureuser
	IdentityFile ~/.ssh/vm-rmsv_key.pem
	StrictHostKeyChecking=no
	AddKeysToAgent yes
	IdentitiesOnly=yes
	ForwardX11=yes
	ForwardX11Trusted=yes

# ./1azureagent1/.
